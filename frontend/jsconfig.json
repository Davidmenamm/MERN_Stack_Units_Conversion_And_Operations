{
  "allowJs": true,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@styles/*": ["./src/styles/*"],
      "@containers/*": ["./src/containers/*"],
      "@utils/*": ["./src/utils/*"],
      "@pages/*": ["./src/pages/*"],
      "@features/*": ["./src/features/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
